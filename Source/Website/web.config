  <system.web>
    <machineKey validationKey="6DE0A9FC92B74E245AAC92A3A18E4A05CE40E9BB" decryptionKey="B4F1C1E10C67897FBDBC67E60C977457DF79EF8D2315AAB0" decryption="3DES" validation="SHA1" />
